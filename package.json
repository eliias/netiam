{
  "name": "netiam",
  "description": "REST API library",
  "main": "lib/netiam.js",
  "scripts": {
    "build": "./bin/build",
    "clean": "./bin/clean",
    "lint": "./bin/lint",
    "start": "npm run watch",
    "test": "npm run test:unit",
    "test:debug": "DEBUG=netiam:* npm test",
    "test:unit": "mocha",
    "test:watch": "mocha --watch",
    "test:coverage": "istanbul cover _mocha --report lcovonly -- -R spec",
    "watch": "./bin/watch",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "dependencies": {
    "bluebird": "^3.1.1",
    "lodash": "^4.0.0",
    "netiam-errors": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015-node5": "^1.1.0",
    "codeclimate-test-reporter": "0.3.0",
    "eslint": "1.10.1",
    "express": "4.13.4",
    "istanbul": "0.4.2",
    "mocha": "^2.3.4",
    "semantic-release": "4.3.5",
    "should": "^8.0.1",
    "supertest": "1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/netiam/netiam.git"
  },
  "keywords": [
    "REST",
    "RESTful",
    "Microservice",
    "Webservice",
    "API",
    "OData",
    "Library"
  ],
  "author": "Hannes Moser <hannes@impossiblearts.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/netiam/netiam/issues"
  },
  "homepage": "https://github.com/netiam/netiam",
  "engines": {
    "node": ">=5",
    "npm": ">=3"
  },
  "publishConfig": {
    "tag": "next"
  },
  "release": {
    "branch": "v7"
  }
}
