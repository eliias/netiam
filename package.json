{
  "name": "netiam",
  "description": "REST API library",
  "main": "lib/netiam.js",
  "scripts": {
    "build": "./bin/build",
    "clean": "./bin/clean",
    "lint": "./bin/lint",
    "start": "npm run watch",
    "test": "npm run test:unit",
    "test:debug": "DEBUG=netiam:* npm test",
    "test:unit": "mocha",
    "test:watch": "mocha --watch",
    "test:coverage": "istanbul cover _mocha --report lcovonly -- -R spec",
    "watch": "./bin/watch",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "dependencies": {
    "debug": "^3.0.0",
    "lodash": "^4.16.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "codeclimate-test-reporter": "0.4.1",
    "eslint": "^4.1.0",
    "express": "5.0.0-alpha.6",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^5.0.0",
    "semantic-release": "12.4.1",
    "should": "^13.0.0",
    "supertest": "3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/netiam/netiam.git"
  },
  "keywords": [
    "REST",
    "RESTful",
    "Microservice",
    "Webservice",
    "API",
    "OData",
    "Library"
  ],
  "author": "Hannes Moser <hannes@impossiblearts.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/netiam/netiam/issues"
  },
  "homepage": "https://github.com/netiam/netiam",
  "engines": {
    "node": ">=9",
    "npm": ">=5"
  },
  "publishConfig": {
    "tag": "latest"
  }
}
